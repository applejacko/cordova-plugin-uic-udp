<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="edu.uic.updtransmit" version="0.0.1">
    
	
	<name>UDPTransmit</name>
    <description>Cordova UDP Transmit Plugin</description>
    <license>IDOT/UIC</license>
    <keywords>cordova,UDP,udp</keywords>
	
	
	<js-module src="www/UDPTransmit.js" name="updtransmit">
        <clobbers target="udptransmit" />
    </js-module>
    
	
	<platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="UDPTransmit">
                <param name="ios-package" value="CDVUDPTransmit"/>
            </feature>
        </config-file>
        <header-file src="src/ios/UDPTransmit.h" />
        <source-file src="src/ios/UDPTransmit.m" />
	</platform>
    
	
	<platform name="android">
        <config-file target="config.xml" parent="/*">
			<feature name="UDPTransmit">
				<param name="android-package" value="CDVUDPTransmit"/>
			</feature>
        </config-file>
        <source-file src="src/android/UDPTransmit.java" />
	</platform>
	
	<!-----------------------------  -------------------------------------------------->
	
</plugin>
